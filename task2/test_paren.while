{-
 Tests parser handling of brackets and comparison operators
 Author: Youssef Amin
-}

x := input;
y := 5;

-- Standard comparison inside brackets
if [x < 10] then
    a := 1
else
    a := 0
fi;

-- Comparison with spaces omitted
if [x>=10] then
    b := 2
else
    b := 0
fi;

-- Comparison inside not [...] brackets
if not [x = y] then
    c := 3
else
    c := 4
fi;

-- Combined and/or with parenthesized comparisons
if [[[[[[[[x < 10] and [y > 2]] or [x = 0]]]]]]] then
    d := 5
else
    d := 6
fi;

output := a + b + c + d